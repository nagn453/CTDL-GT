def count_word_frequency(words):
    word_count = {}
    for word in words:
        if word in word_count:
            word_count[word] += 1
        else: 
            word_count[word] = 1
    return word_count
    
words = ['apple', 'orange', 'banana', 'apple', 'orange', 'apple']
count = count_word_frequency(words)
print(count)
